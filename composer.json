{
    "name": "johnes-ke/mpesa-php-api",
    "description": "M-pesa PHP API for Laravel Php Framework",
    "keywords": [
        "mpesa",
        "api"
    ],
    "homepage": "https://github.com/johnes-ke/mpesa-php-api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johnes Mecha",
            "email": "hi@johnestech.com",
            "homepage": "https://johnestech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "vlucas/phpdotenv" : "^5.4.1"
    },
    "autoload": {
        "psr-4": {
          "JohnesKe\\MpesaPhpApi\\": "src"
      }
  },
  "autoload-dev": {
        "psr-4": {
            "JohnesKe\\MpesaPhpApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohnesKe\\MpesaPhpApi\\MpesaApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
