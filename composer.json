{
    "name": "johnes-ke/mpesa-php-api",
    "description": "M-pesa Daraje Php API for Laravel Framework",
    "keywords": ["mpesa","api","laravel","framework"],
    "homepage": "https://github.com/johnes-ke/mpesa-php-api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johnes Mecha",
            "email": "jmecha09@gmail.com",
            "homepage": "https://johnestech.com",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "vlucas/phpdotenv" : "^5.6.0"
    },
    "autoload": {
        "psr-4": {
          "src\\": "src/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohnesKe\\MpesaPhpApi\\MpesaApiServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
