{
    "name": "johnes-ke/mpesa-php-api",
    "description": "Safaricom Daraja M-pesa API for laravel framework",
    "keywords": ["laravel","mpesa","billing"],
    "homepage": "https://github.com/johnes-ke/mpesa-php-api",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "vlucas/phpdotenv": "16.3.1"
    },
    "autoload": {
        "psr-4": {
          "JohnesKe\\MpesaPhpApi\\": "src/"
      }
  },
  "autoload-dev": {
        "psr-4": {
            "JohnesKe\\MpesaPhpApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohnesKe\\MpesaPhpApi\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
